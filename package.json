{
	"name": "ivans-rv-sveltekit",
	"version": "0.0.1",
	"scripts": {
		"prebuild": "esbuild src/remark/importAssets.ts --outfile=./importAssets.js",
		"dev": "npm run prebuild && vite dev",
		"build": "npm run prebuild && vite build",
		"preview": "npm run prebuild && vite preview",
		"preinstall": "npm run prebuild",
		"tsc:watch": "tsc --noEmit --watch",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"postbuild": "npx svelte-sitemap --domain https://ivan.aero",
		"genmeta": "tsx -- src/genmeta.ts"
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "^3.0.1",
		"@sveltejs/kit": "^2.5.0",
		"@types/js-yaml": "^4.0.9",
		"@types/lunr": "^2.3.7",
		"@types/mdast": "^4.0.3",
		"@typescript-eslint/eslint-plugin": "^6.21.0",
		"@typescript-eslint/parser": "^6.21.0",
		"dayjs": "^1.11.10",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"image-size": "^1.1.1",
		"js-yaml": "^4.1.0",
		"lightgallery": "^2.7.2",
		"lunr": "^2.3.9",
		"mdsvex": "^0.11.0",
		"mrmime": "^2.0.0",
		"prettier": "^3.2.5",
		"prettier-plugin-svelte": "^3.2.1",
		"query-string": "^8.2.0",
		"rehype-autolink-headings": "^7.1.0",
		"rehype-slug": "^6.0.0",
		"ress": "^5.0.2",
		"sass": "^1.71.0",
		"sharp": "^0.33.2",
		"svelte": "^4.2.11",
		"svelte-check": "^3.6.4",
		"svelte-preprocess": "^5.1.3",
		"svelte-sitemap": "^2.6.0",
		"tsx": "^4.7.1",
		"tslib": "^2.6.2",
		"typescript": "~5.3.3",
		"vite": "^5.1.3"
	},
	"type": "module"
}
