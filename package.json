{
	"name": "ivans-rv-sveltekit",
	"version": "0.0.1",
	"scripts": {
		"prebuild": "esbuild src/remark/importAssets.ts --outfile=./importAssets.js",
		"dev": "npm run prebuild && vite dev",
		"build": "npm run prebuild && vite build",
		"preview": "npm run prebuild && vite preview",
		"preinstall": "npm run prebuild",
		"tsc:watch": "tsc --noEmit --watch",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"postbuild": "npx svelte-sitemap --domain https://ivan.aero",
		"genmeta": "tsx -- src/genmeta.ts",
		"gen-cleco-caps": "tsx -- tools/cleco-caps.ts",
		"gen-bundles": "node tools/bundles.mjs"
	},
	"optionalDependencies": {
		"@rollup/rollup-linux-x64-gnu": "4.47.1"
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "^3.0.9",
		"@sveltejs/kit": "^2.36.1",
		"@sveltejs/vite-plugin-svelte": "^5.1.1",
		"@types/js-yaml": "^4.0.9",
		"@types/lunr": "^2.3.7",
		"@types/mdast": "^4.0.4",
		"@types/node": "^24.3.0",
		"@typescript-eslint/eslint-plugin": "^8.40.0",
		"@typescript-eslint/parser": "^8.40.0",
		"dayjs": "^1.11.13",
		"eslint": "^9.34.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.11.0",
		"image-size": "^2.0.2",
		"js-yaml": "^4.1.0",
		"lightgallery": "^2.8.3",
		"lunr": "^2.3.9",
		"mdsvex": "^0.12.6",
		"mrmime": "^2.0.1",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"query-string": "^9.2.2",
		"rehype-autolink-headings": "^7.1.0",
		"rehype-slug": "^6.0.0",
		"ress": "^5.0.2",
		"s-expression": "^3.1.1",
		"sass": "^1.90.0",
		"sharp": "^0.34.3",
		"svelte": "^5.38.2",
		"svelte-check": "^4.3.1",
		"svelte-preprocess": "^6.0.3",
		"svelte-sitemap": "^2.7.1",
		"tslib": "^2.8.1",
		"tsx": "^4.20.4",
		"typescript": "~5.9.2",
		"vite": "^6.3.5"
	},
	"type": "module"
}
