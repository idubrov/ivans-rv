{
	"name": "ivans-rv-sveltekit",
	"version": "0.0.1",
	"scripts": {
		"prebuild": "esbuild src/remark/importAssets.ts --outfile=./importAssets.js",
		"dev": "npm run prebuild && vite dev",
		"build": "npm run prebuild && vite build",
		"preview": "npm run prebuild && vite preview",
		"preinstall": "npm run prebuild",
		"tsc:watch": "tsc --noEmit --watch",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"postbuild": "npx svelte-sitemap --domain https://ivan.aero",
		"genmeta": "ts-node --esm -- src/genmeta.ts"
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "^2.0.2",
		"@sveltejs/kit": "^1.22.1",
		"@types/js-yaml": "^4.0.5",
		"@types/lunr": "^2.3.4",
		"@types/mdast": "^4.0.0",
		"@typescript-eslint/eslint-plugin": "^5.61.0",
		"@typescript-eslint/parser": "^5.61.0",
		"dayjs": "^1.11.9",
		"eslint": "^8.44.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte": "^2.32.2",
		"image-size": "^1.0.2",
		"js-yaml": "^4.1.0",
		"lightgallery": "^2.7.1",
		"lunr": "^2.3.9",
		"mdsvex": "^0.11.0",
		"mrmime": "^1.0.1",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"query-string": "^8.1.0",
		"rehype-autolink-headings": "^6.1.1",
		"rehype-slug": "^5.1.0",
		"ress": "^5.0.2",
		"sass": "^1.63.6",
		"sharp": "^0.32.4",
		"svelte": "^4.0.5",
		"svelte-check": "^3.4.5",
		"svelte-preprocess": "^5.0.4",
		"svelte-sitemap": "^2.6.0",
		"ts-node": "^10.9.1",
		"tslib": "^2.6.0",
		"typescript": "~5.1.6",
		"vite": "^4.4.2"
	},
	"type": "module"
}
